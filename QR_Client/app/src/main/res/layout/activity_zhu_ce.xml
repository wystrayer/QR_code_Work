<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.er_wei_ma_3.zhu_ce">
    <TextView
        android:id="@+id/textView9"
        android:layout_width="368dp"
        android:layout_height="47dp"
        android:textColor="#ffffff"
        android:text="注册"
        android:gravity="center"
        android:background="#4889ca"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="基本信息"
        android:gravity="center"
        android:textSize="18dp"
        android:textColor="#000000"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="55dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   用 户 名"
        android:gravity="center_vertical"
        android:textSize="18dp"
        android:background="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="80dp" />
    <EditText
        android:id="@+id/yhm"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:maxLength="30"
        android:gravity="center_vertical"
        android:hint="字母、数字或“_”，6-30位"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        android:layout_marginLeft="0dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="80dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   密    码"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"

        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />
    <EditText
        android:id="@+id/mm"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:inputType="textPassword"
        android:gravity="center_vertical"
        android:hint="不少于6位"
        app:layout_constraintLeft_toRightOf="@+id/textView12"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/yhm" />
    <TextView
        android:id="@+id/tmmqr"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   性    别"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />
    <EditText
        android:id="@+id/xb"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:inputType="textPassword"
        android:gravity="center_vertical"
        android:hint="请输入性别"
        app:layout_constraintLeft_toRightOf="@+id/tmmqr"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/mm" />

    <TextView
        android:id="@+id/xxxx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="详细信息"
        android:gravity="center"
        android:textSize="18dp"
        android:textColor="#000000"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="212dp" />
    <TextView
        android:id="@+id/txq"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   兴    趣"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/xxxx" />
    <EditText
        android:id="@+id/xq"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:hint="请输入您的兴趣，以便为您服务"
        app:layout_constraintLeft_toRightOf="@+id/txq"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/xxxx" />
    <TextView
        android:id="@+id/zzlx"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   联系方式"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/txq" />
    <TextView
        android:id="@+id/sfz"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:text="QQ号码"
        android:textColor="#000000"
        app:layout_constraintLeft_toRightOf="@+id/zzlx"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/xq" />
    <TextView
        android:id="@+id/tzzhm"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   QQ号码"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/zzlx" />
    <EditText
        android:id="@+id/zzhm"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:hint="请准确完整填写"
        app:layout_constraintLeft_toRightOf="@+id/tzzhm"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/sfz" />
    <TextView
        android:id="@+id/tsjhm"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   手机号码"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/tzzhm" />
    <EditText
        android:id="@+id/sjhm"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:hint="请准确填写您的手机号码"
        app:layout_constraintLeft_toRightOf="@+id/tsjhm"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/zzhm" />
    <TextView
        android:id="@+id/tdzyj"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="   电子邮件"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/tsjhm" />
    <EditText
        android:id="@+id/dzyj"
        android:layout_width="248dp"
        android:layout_height="40dp"
        android:background="#ffffff"
        android:textSize="18dp"
        android:gravity="center_vertical"
        android:hint="请准确填写邮箱地址"
        app:layout_constraintLeft_toRightOf="@+id/tdzyj"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/sjhm" />

    <Button
        android:id="@+id/xyb"
        android:layout_width="350dp"
        android:layout_height="35dp"
        android:text="下一步"
        android:onClick="xiayibu"
        android:textColor="#ffffff"
        android:background="#0072e3"
        android:gravity="center"
        android:layout_marginLeft="4dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/dzyj" />


</android.support.constraint.ConstraintLayout>
